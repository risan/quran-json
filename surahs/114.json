{"number":114,"name":"النَّاسِ","transliteration":"An-Naas","translation":"Mankind","revelation_type":"Meccan","total_verses":6,"verses":[{"number":1,"text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ النَّاسِ"},{"number":2,"text":"مَلِكِ النَّاسِ"},{"number":3,"text":"إِلَٰهِ النَّاسِ"},{"number":4,"text":"مِنْ شَرِّ الْوَسْوَاسِ الْخَنَّاسِ"},{"number":5,"text":"الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ"},{"number":6,"text":"مِنَ الْجِنَّةِ وَالنَّاسِ"}]}
